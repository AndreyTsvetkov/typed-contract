{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../typed-contract.js",
    "sourceMap": true,
    "declaration": true
  },
  "FilesGlob":[
    "**/*.ts"
  ],
  "files": [
    "Contract.ts"
  ],
  "exclude": [
    "node_modules",
    "typings"
  ]
}